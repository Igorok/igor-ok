<!doctype html>
<html lang="ru">
    <head>
        <meta charset="utf-8">
        <title>
            Igor ok {% block title %}{% endblock title %}
        </title>

        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% block meta %}{% endblock meta %}
        <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/blog/css/bootstrap.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/blog/css/style.css') }}" />
        {% block style %}{% endblock style %}
        <!-- Scripts -->
        <script type="text/javascript" src="{{ asset('bundles/blog/js/jquery.min.js') }}"></script>
        <script src="{{ asset('bundles/blog/js/bootstrap.min.js') }}"></script>
        <script type="text/javascript">
            $(function(){
                // scroll header
                $(window).scroll(function () {
                    var scrollHeight = $('body, html').scrollTop();
                    var scrollWebkitHeight = $('body').scrollTop();
                    
                    if (scrollWebkitHeight > 700 || scrollHeight > 700 ) {
                        $('.scrollTopButton').fadeIn();
                    }
                    else {
                        $('.scrollTopButton').fadeOut();
                    }
                });
                // top scroll
                $('.scrollTopButton').click(function(){
                    $('html,body').animate({scrollTop:0}, 1000);
                });
            });
        </script>
        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
          <script src="{{ asset('bundles/blog/js/html5shiv.js') }}"></script>
          <script src="{{ asset('bundles/blog/js/respond.min.js') }}"></script>
        <![endif]-->

        {% block script %}{% endblock script %}
    
    

<body>
        <div id="wrap" class="container-fluid">
            <!-- Fixed navbar -->
            <div class="container">
              <!-- mobile logo -->
              <a class="mobileLogo" href="{{ path('_default') }}">
                <img src="{{ asset('bundles/blog/img/logo.png') }}" alt="">
              </a>
              <!-- header start -->
              <div class="navbar navbar-inverse">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="{{ path('_default') }}">
                        <img src="{{ asset('bundles/blog/img/logo.png') }}" alt="">
                    </a>
                </div>
                  
                <div class="navbar-collapse collapse">   
                    <div class="row">
                    <div class="knp_menu navbar-left clearfix">
                    {{ knp_menu_render('DemosBlogBundle:Builder:leftMenu', {'class':'nav'}) }}
                    </div>
                    <div class="knp_menu navbar-right clearfix">
                    {{ knp_menu_render('DemosBlogBundle:Builder:rightMenu', {'class':'nav'}) }}
                    </div>
                    </div>
                    
                </div><!--/.nav-collapse -->
              </div>
              <!-- header end -->
              
              
            <!-- content|raw is required for phtml fallback -->
            {% block content %}{{ content|raw }}{% endblock content %}
            
            
        {% block inline %}{% endblock inline %}
        
          
            </div>
            <!-- -->
        </div>
        <!-- footer -->
        <div id="footer" class="container">
          <div class="container">
            <div class="copyRight">
                &copy; ipatov-soft.ru
            </div>
            <!-- -->
           

          </div>
        </div>
        <!-- end footer -->

        <!-- scroll top -->
        <div class="btn btn-default btn-lg scrollTopButton">
            <span class="glyphicon glyphicon-chevron-up"></span>
        </div>
        <!-- end scroll top -->
        
    </body>
